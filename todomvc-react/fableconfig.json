{
  "projFile": "index.fsx",
  "sourceMaps": true,
  "babelPlugins": ["transform-runtime"],
  "outDir": "out",
  "rollup": {
    "plugins": [
        ["replace", {"process.env.NODE_ENV": "'production'"}],
        ["commonjs", {
            "namedExports": {
                "react": ["Component", "createElement"],
                "react-dom": ["render"]
            }
        }],
        "uglify"
    ]
  },
  "targets": {
      "debug": { "watch": true }
  }
}